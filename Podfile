source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'

#修改为项目依赖方式
install! 'cocoapods', :generate_multiple_pod_projects => true
# 忽略引入库的所有警告
inhibit_all_warnings!


target 'KW_Swift' do
  
  use_frameworks!
  # 快速访问本地资源
  #pod 'R.swift', '~> 5.0'  # https://github.com/mac-cain13/R.swift
  
  pod 'TZImagePickerController'
  # https://github.com/banchichen/TZImagePickerController.git
  
  # autoLayout 自动化布局
  pod 'SnapKit'
  
  # networking 数据请求
  pod 'Moya'
  # Image 下载、缓存网络图片
  pod 'Kingfisher'
  
  # JSON
  pod 'SwiftyJSON'
  pod 'HandyJSON', '~> 5.0.1' # https://github.com/alibaba/HandyJSON
   
  # Objective-C
  pod 'MJRefresh'
  pod 'FDFullscreenPopGesture'
  pod 'DZNEmptyDataSet'
  pod 'MBProgressHUD' # https://github.com/jdg/MBProgressHUD
  
  # Keyboard
  pod 'IQKeyboardManagerSwift', '~> 6.0'  # https://github.com/hackiftekhar/IQKeyboardManager
  
  # 控制器
  pod 'JXSegmentedView'  # https://github.com/pujiaxin33/JXSegmentedView
  pod 'JXPagingView/Paging'
  # 轮播
  pod 'JXBanner'  # https://github.com/Coder-TanJX/JXBanner
  # 图片浏览
  pod 'JXPhotoBrowser', '~> 3.0' # https://github.com/JiongXing/PhotoBrowser
  
  
  
  
  pod 'AlipaySDK-iOS'
  pod 'mob_sharesdk'
  pod 'mob_sharesdk/ShareSDKUI'
  pod 'mob_sharesdk/ShareSDKPlatforms/WeChatFull'

  
end
